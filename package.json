{
  "name": "todo_list",
  "version": "1.0.0",
  "description": "직접 만들어보는 To Do List - Express.js + React.js + SQLite",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "to do list",
    "express",
    "react",
    "sqlite",
    "투두리스트",
    "익스프레스",
    "리액트"
  ],
  "author": "hyunki, bae",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  }
}
